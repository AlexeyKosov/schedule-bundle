<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Zenstruck\ScheduleBundle\Schedule\Extension\Handler\EmailHandler">
            <argument /> <!-- MailerInterface -->
            <argument>%zenstruck_schedule.email_handler.default_from%</argument>
            <argument>%zenstruck_schedule.email_handler.default_to%</argument>
            <tag name="schedule.extension_handler" />
        </service>
    </services>
</container>
